{
  "name": "trendyminds/orbit-craft",
  "type": "craft-plugin",
  "version": "0.1.0",
  "license": "MIT",
  "authors": [
    {
      "name": "TrendyMinds",
      "homepage": "https://trendyminds.com"
    }
  ],
  "require": {
		"php": "^8.1",
    "craftcms/cms": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "trendyminds\\orbit\\": "src/"
    }
  },
	"extra": {
    "name": "Orbit",
    "handle": "orbit",
    "class": "trendyminds\\orbit\\Orbit",
    "changelogUrl": "https://raw.githubusercontent.com/trendyminds/orbit-craft/main/CHANGELOG.md"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": false,
      "craftcms/plugin-installer": false
    }
  },
  "require-dev": {
    "laravel/pint": "^1.10"
  }
}
